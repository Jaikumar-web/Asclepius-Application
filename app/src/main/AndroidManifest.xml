<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asclepius">

    <uses-permission android:name="com.android.alarm.permission.SET_ALARM"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/iconcenter"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Asclepius">
        <activity android:name=".Skincare"></activity>
        <activity android:name=".LoginApp" />
        <activity android:name=".Booking" />
        <activity android:name=".DoctorApp" />
        <activity android:name=".formQuery" />
        <activity android:name=".medicineTime" />
        <activity android:name=".faq" />
        <activity android:name=".medicare" />
        <activity android:name=".Dashboard" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>